{
    "name": "roxayl/mondegc",
    "description": "Le site du Monde de Generation City",
    "type": "project",
    "keywords": ["generation city", "php", "laravel", "roleplay"],
    "homepage": "https://generation-city.com/monde/",
    "license": "proprietary",
    "authors": [
        {
            "name": "AlfredVallamir",
            "role": "Developer",
            "homepage": "https://www.vallamir-co.fr/index.html"
        }, {
            "name": "Calimero",
            "role": "Developer"
        }, {
            "name": "Lesime",
            "role": "Graphic Designer"
        }, {
            "name": "Myname",
            "role": "Developer"
        }, {
            "name": "Roxel Lay",
            "role": "Main Developer",
            "email": "contact@romukulot.fr",
            "homepage": "https://roxayl.fr"
        }, {
            "name": "Sakuro",
            "role": "Developer"
        }
    ],
    "support": {
        "forum": "https://www.forum-gc.com/",
        "source": "https://bitbucket.org/Roxayl/mondegc"
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^2.10",
        "ezyang/htmlpurifier": "^4.13",
        "fideloper/proxy": "^4.0",
        "funkjedi/composer-include-files": "^1.1",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.2",
        "spatie/laravel-searchable": "^1.6",
        "tormjens/eventy": "^0.7.0",
        "ylsideas/feature-flags": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "2.9.1",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.4",
        "kitloong/laravel-migrations-generator": "^4.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.5",
        "reliese/laravel": "^1.1",
        "vimeo/psalm": "^4.11"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "include_files": [
            "app/Overrides/Foundation/helpers.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "GenCity\\": "legacy/php/GenCity/",
            "Squirrel\\": "lib/Squirrel/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@php artisan monde:override-vendor"
        ],
        "post-update-cmd": [
            "@php artisan monde:override-vendor"
        ],
        "post-autoload-dump": [
            "@php artisan monde:override-vendor",
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
