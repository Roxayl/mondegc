{
    "name": "roxayl/mondegc",
    "description": "Le site du Monde de Generation City",
    "type": "project",
    "keywords": ["generation city", "laravel", "roleplay"],
    "homepage": "https://generation-city.com/monde/",
    "license": "proprietary",
    "authors": [
        {
            "name": "AlfredVallamir",
            "role": "Developer",
            "homepage": "https://www.vallamir-co.fr/index.html"
        }, {
            "name": "Calimero",
            "role": "Developer"
        }, {
            "name": "Lesime",
            "role": "Graphic Designer"
        }, {
            "name": "Myname",
            "role": "Developer"
        }, {
            "name": "Roxayl",
            "role": "Main Developer",
            "email": "contact@roxayl.fr",
            "homepage": "https://roxayl.fr"
        }, {
            "name": "Sakuro",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "contact@roxayl.fr",
        "forum": "https://www.forum-gc.com/",
        "source": "https://github.com/Roxayl/mondegc",
        "chat": "https://discord.gg/4P3HqVbbgR"
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "caxy/php-htmldiff": "^0.1.14",
        "doctrine/dbal": "^2.13",
        "ezyang/htmlpurifier": "^4.16",
        "funkjedi/composer-include-files": "^1.1",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.2",
        "laravel/tinker": "^2.8",
        "mpociot/versionable": "^4.3",
        "simplehtmldom/simplehtmldom": "^2.0@RC",
        "spatie/laravel-searchable": "^1.11",
        "tormjens/eventy": "^0.8.0",
        "ylsideas/feature-flags": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "beyondcode/laravel-dump-server": "^1.9",
        "brianium/paratest": "^6.9",
        "fakerphp/faker": "^1.21",
        "kitloong/laravel-migrations-generator": "^6.8",
        "knuckleswtf/scribe": "^4.16",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.4",
        "phpunit/phpunit": "^9.6",
        "psalm/plugin-laravel": "^2.7",
        "reliese/laravel": "^1.1",
        "spatie/laravel-ignition": "^1.6",
        "vimeo/psalm": "^4.30"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "funkjedi/composer-include-files": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "include_files": [
            "app/Overrides/Foundation/helpers.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "GenCity\\": "legacy/php/GenCity/",
            "Roxayl\\MondeGC\\": "app/",
            "Squirrel\\": "libs/Squirrel/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@php artisan monde:override-vendor"
        ],
        "post-update-cmd": [
            "@php artisan monde:override-vendor"
        ],
        "pre-autoload-dump": [
            "@php -r \"unlink('bootstrap/cache/packages.php') && unlink('bootstrap/cache/services.php');\""
        ],
        "post-autoload-dump": [
            "@php artisan monde:override-vendor",
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
