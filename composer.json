{
    "name": "generationcity/mondegc",
    "description": "Le site du Monde de Generation City",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Roxel Lay",
            "email": "contact@romukulot.fr",
            "homepage": "https://romukulot.fr",
            "role": "Developer"
        }, {
            "name": "Calimero",
            "role": "Developer"
        }, {
            "name": "Sakuro",
            "role": "Developer"
        }, {
            "name": "Lesime",
            "role": "Graphiste"
        }
    ],
    "support": {
        "forum": "https://www.forum-gc.com/"
    },
    "require": {
        "php": "^7.4",
        "brackets/craftable": "^5.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "reliese/laravel": "^0.0.16",
        "spatie/laravel-searchable": "^1.6"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "brackets/admin-generator": "^5.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
